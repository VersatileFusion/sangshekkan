{
	"exclude": [
		".react-router/types/**/*",
		"test/**/*",
		"src/server/**/*",
		"src/__create/**/*"
	],
	"compilerOptions": {
		"lib": ["DOM", "DOM.Iterable", "ES2022"],
		"types": ["node", "vite/client"],
		"target": "ES2022",
		"module": "ES2022",
		"moduleResolution": "bundler",
		"jsx": "react-jsx",
		"rootDirs": [".", "./.react-router/types"],
		"baseUrl": ".",
		"esModuleInterop": true,
		"verbatimModuleSyntax": true,
		"noEmit": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": false,
		"noImplicitAny": false,
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
